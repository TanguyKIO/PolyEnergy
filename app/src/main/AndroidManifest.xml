            android:theme="@style/Theme.PolyEnergy.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyA6Sbv6Es-0CEEYZ_frHy9t_vTxGOLWWTY" />
    </application>

</manifest>